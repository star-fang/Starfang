<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <EditTextPreference
        android:title="DB SYNC 주소 입력"
        android:key="text_address"
        android:defaultValue = "http://192.168.25.4"
        />


    <ListPreference
        android:defaultValue="0"
        android:entries="@array/pref_list_table"
        android:entryValues="@array/pref_table_list_values"
        android:key="table_list"
        android:negativeButtonText="@null"
        android:positiveButtonText="@null"
        android:title="@string/pref_title_sync_table_to_realm" />



    <Preference
        android:key = "show_data_key"
        android:title="DATA" />

    <Preference
        android:key = "start_sync_key"
        android:title="SYNC" />

    <Preference
        android:key = "start_sync_key_all"
        android:title="SYNC ALL" />


</PreferenceScreen>
